    <title>Login</title>
</head>

<body>
  <div class="container mx-auto d-flex align-item-center">
  <div class="card">
    <div class="row">
      <div class="col-xl-6 col-lg-6 col-md-6 col-12"><img src="/img/luis-covarrubias-veIJKyuBk3Q-unsplash.jpg" alt="" class="img-fluid login-img"></div>
      <div class="col-xl-6 col-lg-6 col-md-6 col-12 d-flex align-items-center justify-content-center">
        <div class="login-form">
        <div class="form" >
          <div class="get-started">
            <p class="text">Get Started
          </div>
          <div class="sign-text">
            <p class="sign-up-text">Don't have an account?<a href="/signup">  Sign Up</a>
            <% if(messages.error) {%>
              <h6 style="color:rgb(235, 28, 21)">
                *<%= messages.error %>
                  <% } %></p>
          </div>
          <form action="/login" method="post">
              </h6>
            <input name="email" type="email" placeholder="Email" class="input-phone">
            <br>
            <input name="password" type="password" placeholder="Password" class="input-password">
            <br>
            <button class="login-btn" type="submit">LOGIN</button>
          </form>
         <p class="add-ch-login"><a href="/" class="add-ch-login-a">Home</a></p>
        </div>
      </div>
    </div>
    </div>
  </div>
</div>
